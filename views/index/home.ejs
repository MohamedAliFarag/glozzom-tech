<!--NewsLetter-->

<section id="newsletter" class="text-center p-5 bg-dark text-white">
  <div class="container">
    <div class="row">
      <div class="col">
        <h1>Sign Up For Newsletter</h1>
        <%if(success && success.length>0){%>
          <div id="flash" class="alert alert-success" role="alert">
            <%=success%>
          </div>
          <%}%>
          <%if(error && error.length>0){%>
          <div id="flash" class="alert alert-warning" role="alert">
            <%=error%>
          </div>
          <%}%>
        <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Ab tenetur aliquid eos quaerat hic eligendi.</p>
        <form action="/#newsletter" method="POST" class="form-inline justify-content-center" >
          <input type="text" class="form-control mb-2 mr-2" placeholder="Enter Name" name="name" value="<%=oldInput.name%>">
          <input type="email" class="form-control mb-2 mr-2" placeholder="Enter Email" name="email" value="<%=oldInput.email%>">
          <button class="btn btn-primary mb-2 mr-2">Subscribe</button>
        </form>
      </div>
    </div>
  </div>
</section>



